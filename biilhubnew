-- üìå Main UI Setup
local ScreenGui = Instance.new("ScreenGui", game.CoreGui)

-- üîµ Tombol Bulat B untuk membuka menu
local OpenButton = Instance.new("TextButton", ScreenGui)
OpenButton.Size = UDim2.new(0, 60, 0, 60)
OpenButton.Position = UDim2.new(0.05, 0, 0.3, 0)
OpenButton.Text = "B"
OpenButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
OpenButton.TextColor3 = Color3.new(1,1,1)
OpenButton.Font = Enum.Font.SourceSansBold
OpenButton.TextSize = 24
Instance.new("UICorner", OpenButton)

-- ü™ü Menu utama (tampil saat tombol B ditekan)
local MainFrame = Instance.new("Frame", ScreenGui)
MainFrame.Size = UDim2.new(0, 300, 0, 350)
MainFrame.Position = UDim2.new(0.15, 0, 0.3, 0)
MainFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
MainFrame.Visible = false
Instance.new("UICorner", MainFrame)

-- üéõÔ∏è Tombol Auto Farm
local ToggleButton = Instance.new("TextButton", MainFrame)
ToggleButton.Size = UDim2.new(0, 250, 0, 50)
ToggleButton.Position = UDim2.new(0.08, 0, 0.05, 0)
ToggleButton.Text = "OFF"
ToggleButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
ToggleButton.TextColor3 = Color3.new(1,1,1)
Instance.new("UICorner", ToggleButton)

-- Status Auto Farm
local autoFarm = false
local delayTime = 15

ToggleButton.MouseButton1Click:Connect(function()
    autoFarm = not autoFarm
    ToggleButton.Text = autoFarm and "ON" or "OFF"
    ToggleButton.BackgroundColor3 = autoFarm and Color3.fromRGB(0,255,0) or Color3.fromRGB(255,0,0)
end)

-- üó∫ Waypoint List (langsung teleport ke titik pilihan)
local waypoints = {
    ["MANAHAN"] = Vector3.new(-5111, 5, -3760),
    ["COURIER"] = Vector3.new(-6795, 3, -444),
    ["SPBU"] = Vector3.new(-17053, 104, 6567),
    ["BANK"] = Vector3.new(-6741, 3, 2975),
    ["PASAR"] = Vector3.new(-8777, 3, 644),
    ["RUMAH"] = Vector3.new(-8382, 2, -3822)
}

-- üõ∞ Fungsi Teleport + Auto Tekan F
local function teleportTo(position)
    local player = game.Players.LocalPlayer
    if player and player.Character then
        player.Character:MoveTo(position)

        -- üîò Auto Tekan F (ambil item / interaksi)
        local vim = game:GetService("VirtualInputManager")
        task.wait(1)
        pcall(function()
            vim:SendKeyEvent(true, Enum.KeyCode.F, false, game)
            task.wait(0.3)
            vim:SendKeyEvent(false, Enum.KeyCode.F, false, game)
        end)
    end
end

-- üì¶ Generate Tombol Teleport
local yPos = 0.2
for name, pos in pairs(waypoints) do
    local btn = Instance.new("TextButton", MainFrame)
    btn.Size = UDim2.new(0, 250, 0, 40)
    btn.Position = UDim2.new(0.08, 0, yPos, 0)
    btn.Text = "Teleport: "..name
    btn.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
    btn.TextColor3 = Color3.new(1, 1, 1)
    Instance.new("UICorner", btn)

    btn.MouseButton1Click:Connect(function()
        if autoFarm then
            teleportTo(pos)
            task.wait(delayTime)
        else
            teleportTo(pos)
        end
    end)

    yPos = yPos + 0.13
end

-- üéØ Buka/Tutup Menu pakai tombol B
OpenButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible
end)
