local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local root = char:WaitForChild("HumanoidRootPart")

-- Lokasi-lokasi yang ingin dikunjungi
local locations = {
    Vector3.new(2879, 4, -863),   -- Lokasi 1
    Vector3.new(-5, 4, 25),       -- Lokasi 2
    Vector3.new(3141, 5, -111)    -- Lokasi 3
}

local promptName = "collect"      -- <=== Nama prompt yang ditekan
local teleportDelay = 2           -- Delay antar teleport

-- Fungsi teleport
local function teleportTo(position)
    root.CFrame = CFrame.new(position)
end

-- Menekan ProximityPrompt otomatis
local function pressCollect()
    -- Mencari prompt di seluruh Workspace
    local prompt = workspace:FindFirstChild(promptName, true)

    if prompt and prompt:IsA("ProximityPrompt") then
        fireproximityprompt(prompt)
        print("Menekan prompt:", prompt.Name)
    else
        warn("Prompt 'collect' tidak ditemukan.")
    end
end

-- Loop terus-menerus
while true do
    for _, location in ipairs(locations) do
        teleportTo(location)
        task.wait(0.5)    -- beri waktu sampai tubuh stabil

        pressCollect()    -- tekan E berbasis ProximityPrompt

        task.wait(teleportDelay)
    end
end
