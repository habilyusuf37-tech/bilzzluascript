-- PREMIUM BY MIYOSHI - MINI PREMIUM EDITION (300x460)

local CorrectKey = "MIYOSHI2025"

local Players = game:GetService("Players")
local CoreGui = game:GetService("CoreGui")
local TweenService = game:GetService("TweenService")
local Workspace = game:GetService("Workspace")

-- ====================== REDEEM KEY ======================
local RedeemGui = Instance.new("ScreenGui", CoreGui)
RedeemGui.ResetOnSpawn = false

local Back = Instance.new("Frame", RedeemGui)
Back.Size = UDim2.new(0, 380, 0, 220)
Back.Position = UDim2.new(0.5, -190, 0.5, -110)
Back.BackgroundColor3 = Color3.fromRGB(15,15,15)
Instance.new("UICorner", Back).CornerRadius = UDim.new(0,16)

local Title = Instance.new("TextLabel", Back)
Title.Size = UDim2.new(1,0,0,50)
Title.BackgroundTransparency = 1
Title.Text = "PREMIUM BY MIYOSHI"
Title.TextColor3 = Color3.fromRGB(255,50,150)
Title.Font = Enum.Font.GothamBlack
Title.TextSize = 28

local TB = Instance.new("TextBox", Back)
TB.Size = UDim2.new(0.85,0,0,45)
TB.Position = UDim2.new(0.075,0,0,70)
TB.PlaceholderText = "Enter Key..."
TB.BackgroundColor3 = Color3.fromRGB(40,40,40)
TB.TextColor3 = Color3.new(1,1,1)
Instance.new("UICorner", TB).CornerRadius = UDim.new(0,10)

local RedeemBtn = Instance.new("TextButton", Back)
RedeemBtn.Size = UDim2.new(0.85,0,0,48)
RedeemBtn.Position = UDim2.new(0.075,0,0,130)
RedeemBtn.BackgroundColor3 = Color3.fromRGB(255,50,150)
RedeemBtn.Text = "REDEEM"
RedeemBtn.TextColor3 = Color3.new(1,1,1)
RedeemBtn.Font = Enum.Font.GothamBlack
Instance.new("UICorner", RedeemBtn).CornerRadius = UDim.new(0,10)

RedeemBtn.MouseButton1Click:Connect(function()
    if TB.Text == CorrectKey then
        RedeemGui:Destroy()
        game.StarterGui:SetCore("SendNotification",{Title="MIYOSHI",Text="Mini Premium Activated!",Duration=5})

        -- ========================= MINI PREMIUM UI =========================
        local ScreenGui = Instance.new("ScreenGui", CoreGui)

        -- Main Frame Kecil Premium
        local Main = Instance.new("Frame", ScreenGui)
        Main.Size = UDim2.new(0, 300, 0, 460)  -- KECIL & ELEGAN
        Main.Position = UDim2.new(0, 30, 0.5, -230)
        Main.BackgroundColor3 = Color3.fromRGB(18,18,18)
        Main.BorderSizePixel = 0
        Main.Active = true
        Main.Draggable = true
        Main.Visible = false
        Main.ClipsDescendants = true

        local Corner = Instance.new("UICorner", Main)
        Corner.CornerRadius = UDim.new(0,18)

        -- Gradient + Glow Premium
        local Gradient = Instance.new("UIGradient", Main)
        Gradient.Color = ColorSequence.new{
            ColorSequenceKeypoint.new(0, Color3.fromRGB(40,10,60)),
            ColorSequenceKeypoint.new(1, Color3.fromRGB(10,10,25))
        }
        Gradient.Rotation = 90

        local Stroke = Instance.new("UIStroke", Main)
        Stroke.Color = Color3.fromRGB(255,50,150)
        Stroke.Thickness = 2
        Stroke.Transparency = 0.3

        -- Title Premium
        local Title = Instance.new("TextLabel", Main)
        Title.Size = UDim2.new(1,0,0,50)
        Title.BackgroundTransparency = 1
        Title.Text = "MIYOSHI"
        Title.TextColor3 = Color3.fromRGB(255,80,180)
        Title.Font = Enum.Font.GothamBlack
        Title.TextSize = 24

        -- Bulatan B Premium
        local BBtn = Instance.new("TextButton", ScreenGui)
        BBtn.Size = UDim2.new(0,58,0,58)
        BBtn.Position = UDim2.new(1,-75,0,12)
        BBtn.BackgroundColor3 = Color3.new(1,1,1)
        BBtn.Text = "B"
        BBtn.TextColor3 = Color3.new(0,0,0)
        BBtn.Font = Enum.Font.GothamBlack
        BBtn.TextSize = 36
        BBtn.TextScaled = true
        Instance.new("UICorner", BBtn).CornerRadius = UDim.new(1,0)
        local BStroke = Instance.new("UIStroke", BBtn)
        BStroke.Color = Color3.fromRGB(255,50,150)
        BStroke.Thickness = 3

        BBtn.MouseButton1Click:Connect(function()
            Main.Visible = not Main.Visible
        end)

        -- Scrolling + Buttons
        local Scroll = Instance.new("ScrollingFrame", Main)
        Scroll.Size = UDim2.new(1,-20,1,-65)
        Scroll.Position = UDim2.new(0,10,0,55)
        Scroll.BackgroundTransparency = 1
        Scroll.ScrollBarThickness = 4

        local Layout = Instance.new("UIListLayout", Scroll)
        Layout.Padding = UDim.new(0,10)

        local function Btn(name, callback)
            local b = Instance.new("TextButton", Scroll)
            b.Size = UDim2.new(1,-10,0,48)
            b.BackgroundColor3 = Color3.fromRGB(35,35,35)
            b.Text = name
            b.TextColor3 = Color3.new(1,1,1)
            b.Font = Enum.Font.Gotham
            b.TextSize = 18
            Instance.new("UICorner", b).CornerRadius = UDim.new(0,12)
            local str = Instance.new("UIStroke", b)
            str.Color = Color3.fromRGB(255,50,150)
            str.Thickness = 1
            str.Transparency = 0.7
            b.MouseButton1Click:Connect(function()
                callback()
                b.BackgroundColor3 = Color3.fromRGB(255,50,150)
                task.wait(0.15)
                b.BackgroundColor3 = Color3.fromRGB(35,35,35)
            end)
        end

        -- Fitur
        local AutoFarm = false
        local Coords = {Vector3.new(2879,4,-863), Vector3.new(-5,4,25), Vector3.new(3141,5,-111)}
        local idx = 1
        local deliveries = 0

        Btn("AutoFarm", function() AutoFarm = not AutoFarm end)
        Btn("ESP", function()
            for _, p in Players:GetPlayers() do
                if p ~= Players.LocalPlayer and p.Character then
                    Instance.new("Highlight", p.Character).FillColor = Color3.fromRGB(255,0,0)
                end
            end
        end)
        Btn("Speed 160", function()
            Players.LocalPlayer.Character.Humanoid.WalkSpeed = 160
        end)
        Btn("Hide Name", function()
            for _, v in Players.LocalPlayer.Character:GetDescendants() do
                if v:IsA("BillboardGui") then v:Destroy() end
            end
        end)

        -- AUTOFARM + RADIUS 6000 + TELEPORT TWEEN HALUS
        spawn(function()
            while task.wait(0.8) do
                if AutoFarm and Players.LocalPlayer.Character and Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    local hrp = Players.LocalPlayer.Character.HumanoidRootPart
                    local target = Coords[idx]

                    if deliveries >= 7 then
                        for _, obj in Workspace:GetDescendants() do
                            if obj:IsA("BasePart") and (obj.Name:lower():find("delivery") or obj.Name:lower():find("waypoint") or obj.Name:lower():find("drop")) then
                                if (hrp.Position - obj.Position).Magnitude <= 6000 then
                                    target = obj.Position
                                    break
                                end
                            end
                        end
                    end

                    local tween = TweenService:Create(hrp, TweenInfo.new(0.6), {CFrame = CFrame.new(target + Vector3.new(0,6,0))})
                    hrp.Anchored = true
                    tween:Play()
                    tween.Completed:Wait()
                    hrp.Anchored = false

                    task.wait(0.7)
                    for _, p in Workspace:GetDescendants() do
                        if p:IsA("ProximityPrompt") and (hrp.Position - p.Parent.Position).Magnitude < 30 then
                            fireproximityprompt(p, 5)
                            break
                        end
                    end

                    task.wait(6)
                    deliveries += 1
                    idx = (idx % 3) + 1
                    game.StarterGui:SetCore("SendNotification",{
                        Title = "Delivery #"..deliveries,
                        Text = "+15.000 Cash | +20 Coins",
                        Duration = 4
                    })
                end
            end
        end)

        -- Auto resize
        Layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            Scroll.CanvasSize = UDim2.new(0,0,0,Layout.AbsoluteContentSize.Y + 20)
        end)

        game.StarterGui:SetCore("SendNotification",{
            Title = "MIYOSHI MINI",
            Text = "Premium mini loaded! Klik B",
            Duration = 6
        })

    else
        TB.PlaceholderText = "Key Salah!"
        TB.Text = ""
    end
end)
