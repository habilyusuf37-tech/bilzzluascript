local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local VirtualInputManager = game:GetService("VirtualInputManager")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local root = character:WaitForChild("HumanoidRootPart")
local mouse = player:GetMouse()

-- REDEEM
local Redeemed = false
local Key = "premiumbillzz"
if not Redeemed then
    local sg = Instance.new("ScreenGui", game.CoreGui)
    local fr = Instance.new("Frame", sg)
    fr.Size = UDim2.new(0,340,0,180)
    fr.Position = UDim2.new(0.5,-170,0.5,-90)
    fr.BackgroundColor3 = Color3.fromRGB(12,12,20)
    Instance.new("UICorner",fr).CornerRadius = UDim.new(0,16)
    Instance.new("UIStroke",fr).Color = Color3.fromRGB(0,180,255); Instance.new("UIStroke",fr).Thickness = 2.5

    local txt = Instance.new("TextLabel",fr)
    txt.Size = UDim2.new(1,0,0,40); txt.Text = "BILLZZ PREMIUM"; txt.TextColor3 = Color3.fromRGB(0,220,255)
    txt.Font = Enum.Font.GothamBlack; txt.TextSize = 24; txt.BackgroundTransparency = 1

    local box = Instance.new("TextBox",fr)
    box.Size = UDim2.new(0.8,0,0,40); box.Position = UDim2.new(0.1,0,0.3,0)
    box.PlaceholderText = "Enter key..."; box.BackgroundColor3 = Color3.fromRGB(25,25,45)
    Instance.new("UICorner",box).CornerRadius = UDim.new(0,10)

    local btn = Instance.new("TextButton",fr)
    btn.Size = UDim2.new(0.8,0,0,40); btn.Position = UDim2.new(0.1,0,0.65,0)
    btn.Text = "REDEEM"; btn.BackgroundColor3 = Color3.fromRGB(0,160,255)
    Instance.new("UICorner",btn).CornerRadius = UDim.new(0,10)

    btn.MouseButton1Click:Connect(function()
        if box.Text:lower() == Key then Redeemed = true sg:Destroy() else box.Text = "" box.PlaceholderText = "Wrong key!" task.wait(1.5) box.PlaceholderText = "Enter key..." end
    end)
    repeat task.wait() until Redeemed
end

-- OWNER TAG SEDANG & ELEGAN
local function ownerTag()
    if character:FindFirstChild("Head") then
        for _,v in character.Head:GetChildren() do if v:IsA("BillboardGui") then v:Destroy() end end
        local bill = Instance.new("BillboardGui", character.Head)
        bill.Size = UDim2.new(0,180,0,50); bill.StudsOffset = Vector3.new(0,3,0); bill.AlwaysOnTop = true
        local txt = Instance.new("TextLabel", bill)
        txt.Size = UDim2.new(1,0,1,0); txt.BackgroundTransparency = 1
        txt.Text = "BILLZZ OWNER"; txt.TextColor3 = Color3.fromRGB(255,223,0)
        txt.Font = Enum.Font.GothamBold; txt.TextSize = 28
        txt.TextStrokeTransparency = 0.7
        local glow = Instance.new("UIStroke", txt)
        glow.Color = Color3.fromRGB(255,255,150); glow.Thickness = 2.5; glow.Transparency = 0.4
    end
end
ownerTag()
player.CharacterAdded:Connect(function(c)
    character = c humanoid = c:WaitForChild("Humanoid") root = c:WaitForChild("HumanoidRootPart")
    task.wait(1) ownerTag()
end)

-- CASH NOTIF
local function cashNotif(amount)
    spawn(function()
        local sg = Instance.new("ScreenGui", game.CoreGui)
        local fr = Instance.new("Frame", sg)
        fr.Size = UDim2.new(0,300,0,80); fr.Position = UDim2.new(0.5,-150,-0.2,0)
        fr.BackgroundColor3 = Color3.fromRGB(0,140,0); Instance.new("UICorner",fr).CornerRadius = UDim.new(0,16)
        Instance.new("UIStroke",fr).Color = Color3.fromRGB(0,255,0); Instance.new("UIStroke",fr).Thickness = 3
        local d = Instance.new("TextLabel",fr); d.Size = UDim2.new(0,70,1,0); d.Text = "$"; d.TextColor3 = Color3.new(0,1,0); d.Font = Enum.Font.GothamBlack; d.TextSize = 50; d.BackgroundTransparency = 1
        local t = Instance.new("TextLabel",fr); t.Position = UDim2.new(0,70,0,0); t.Size = UDim2.new(1,-70,1,0); t.BackgroundTransparency = 1
        t.Text = "Earned +"..amount; t.TextColor3 = Color3.new(1,1,1); t.Font = Enum.Font.GothamBold; t.TextSize = 26
        TweenService:Create(fr,TweenInfo.new(0.7),{Position=UDim2.new(0.5,-150,0.15,0)}):Play()
        task.wait(2.3)
        TweenService:Create(fr,TweenInfo.new(0.7),{Position=UDim2.new(0.5,-150,-0.2,0)}):Play()
        task.wait(0.8) sg:Destroy()
    end)
end

