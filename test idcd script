Instance.new("UIStroke",main).Color = Color3.fromRGB(0,190,255); Instance.new("UIStroke",main).Thickness = 2

local title = Instance.new("TextLabel",main)
title.Size = UDim2.new(1,0,0,45); title.Text = "BILLZZ HUB"; title.TextColor3 = Color3.fromRGB(0,220,255)
title.Font = Enum.Font.GothamBlack; title.TextSize = 22; title.BackgroundTransparency = 1

-- Close & Minimize
Instance.new("TextButton",main).Size = UDim2.new(0,32,0,32); Instance.new("TextButton",main).Position = UDim2.new(1,-40,0,6); Instance.new("TextButton",main).Text = "X"; Instance.new("TextButton",main).BackgroundColor3 = Color3.fromRGB(220,50,50)
Instance.new("UICorner",main:FindFirstChild("TextButton"))
main:FindFirstChild("TextButton").MouseButton1Click:Connect(function() gui:Destroy() end)

local mini = Instance.new("TextButton",main)
