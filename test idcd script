-- Variables & Features (sama)
local autofarm = false
local fly = false
local noclip = false
local walkspeed = 16
local jumppower = 50
local delayTime = 8
local flySpeed = 70

-- Waypoints asli (tidak diubah)
local Locations = {
    Vector3.new(2879, 4, -863),
    Vector3.new(-5, 4, 25),
    Vector3.new(3141, 5, -111)
}

-- Autofarm + Cash Notif (sama seperti V3)
local function teleportTo(pos)
    if root and root.Parent then
        root.CFrame = CFrame.new(pos + Vector3.new(0, 5, 0))
        task.wait(0.3)
    end
end

local function holdE()
    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, game)
    task.wait(7.2)
    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, game)
end

local function startAutoFarm()
    if autofarm then return end
    autofarm = true
    task.spawn(function()
        local index = 1
        while autofarm and character and root do
            teleportTo(Locations[index])
            task.wait(0.8)
            holdE()
            local earned = math.random(1800, 3200)
            showCashNotif("$"..string.format("%05d", earned))
            task.wait(delayTime)
            index = index + 1
            if index > #Locations then index = 1 end
        end
    end)
end

