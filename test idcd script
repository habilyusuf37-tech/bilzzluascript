-- BILLZZ PREMIUM HUB V4 - HIDE NAME + OWNER TAG
-- Key: premiumbillzz

local VirtualInputManager = game:GetService("VirtualInputManager")
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local root = character:WaitForChild("HumanoidRootPart")
local mouse = player:GetMouse()

-- === REDEEM SYSTEM ===
local Redeemed = false
local CorrectKey = "premiumbillzz"

if not Redeemed then
    local rg = Instance.new("ScreenGui", game.CoreGui)
    local rf = Instance.new("Frame", rg)
    rf.Size = UDim2.new(0, 340, 0, 180)
    rf.Position = UDim2.new(0.5, -170, 0.5, -90)
    rf.BackgroundColor3 = Color3.fromRGB(12,12,20)
    rf.BorderSizePixel = 0
    Instance.new("UICorner", rf).CornerRadius = UDim.new(0,16)
    local st = Instance.new("UIStroke", rf)
    st.Color = Color3.fromRGB(0,180,255)
    st.Thickness = 2.5

    local title = Instance.new("TextLabel", rf)
    title.Size = UDim2.new(1,0,0,40)
    title.Text = "BILLZZ PREMIUM"
    title.TextColor3 = Color3.fromRGB(0,220,255)
    title.BackgroundTransparency = 1
    title.Font = Enum.Font.GothamBlack
    title.TextSize = 24

    local box = Instance.new("TextBox", rf)
    box.Size = UDim2.new(0.8,0,0,40)
    box.Position = UDim2.new(0.1,0,0.3,0)
    box.PlaceholderText = "Enter key..."
    box.BackgroundColor3 = Color3.fromRGB(25,25,45)
    box.TextColor3 = Color3.new(1,1,1)
    Instance.new("UICorner", box).CornerRadius = UDim.new(0,10)

    local btn = Instance.new("TextButton", rf)
    btn.Size = UDim2.new(0.8,0,0,40)
    btn.Position = UDim2.new(0.1,0,0.65,0)
    btn.Text = "REDEEM"
    btn.BackgroundColor3 = Color3.fromRGB(0,160,255)
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.GothamBold
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0,10)

    btn.MouseButton1Click:Connect(function()
        if box.Text:lower() == CorrectKey then
            Redeemed = true
            rg:Destroy()
        else
            box.Text = ""
            box.PlaceholderText = "Wrong key!"
            task.wait(1.5)
            box.PlaceholderText = "Enter key..."
        end
    end)

    repeat task.wait() until Redeemed
end

-- === HIDE NAME + CUSTOM OWNER TAG ===
local function setupNameTag()
    if character:FindFirstChild("Head") then
        -- Hapus nama asli
        for _, v in pairs(character.Head:GetChildren()) do
            if v:IsA("BillboardGui") or v:IsA("SurfaceGui") then
                v:Destroy()
            end
        end

        -- Buat BillboardGui baru (OWNER TAG)
        local bill = Instance.new("BillboardGui")
        bill.Size = UDim2.new(0, 200, 0, 60)
        bill.StudsOffset = Vector3.new(0, 3, 0)
        bill.AlwaysOnTop = true
        bill.Adornee = character.Head
        bill.Parent = character.Head

        local text = Instance.new("TextLabel", bill)
        text.Size = UDim2.new(1, 0, 1, 0)
        text.BackgroundTransparency = 1
        text.Text = "BILLZZ OWNER"
        text.TextColor3 = Color3.fromRGB(255, 215, 0)  -- Kuning emas
        text.Font = Enum.Font.GothamBlack
        text.TextStrokeTransparency = 0
        text.TextStrokeColor3 = Color3.new(0,0,0)
        text.TextSize = 38
        text.TextScaled = false

        -- Glow effect
        local glow = Instance.new("UIStroke", text)
        glow.Color = Color3.fromRGB(255, 255, 100)
        glow.Thickness = 4
        glow.Transparency = 0.3

        local glow2 = Instance.new("UIStroke", text)
        glow2.Color = Color3.fromRGB(255, 255, 0)
        glow2.Thickness = 8
        glow2.Transparency = 0.7
    end
end

-- Jalankan saat karakter spawn & respawn
setupNameTag()
player.CharacterAdded:Connect(function(char)
    character = char
    humanoid = char:WaitForChild("Humanoid")
    root = char:WaitForChild("HumanoidRootPart")
    task.wait(1)
    setupNameTag()
end)

-- === CASH NOTIFIKASI ===
local function showCashNotif(amount)
    spawn(function()
        local notif = Instance.new("ScreenGui", game.CoreGui)
        local frame = Instance.new("Frame", notif)
        frame.Size = UDim2.new(0, 290, 0, 75)
        frame.Position = UDim2.new(0.5, -145, 0, -100)
        frame.BackgroundColor3 = Color3.fromRGB(0, 140, 0)
        frame.BorderSizePixel = 0
        Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 16)

        local stroke = Instance.new("UIStroke", frame)
        stroke.Color = Color3.fromRGB(0, 255, 0)
        stroke.Thickness = 3

        local dollar = Instance.new("TextLabel", frame)
        dollar.Size = UDim2.new(0, 60, 1, 0)
        dollar.Text = "$"
        dollar.TextColor3 = Color3.new(0,1,0)
        dollar.Font = Enum.Font.GothamBlack
        dollar.TextSize = 48
        dollar.BackgroundTransparency = 1

        local txt = Instance.new("TextLabel", frame)
        txt.Position = UDim2.new(0, 60, 0, 0)
        txt.Size = UDim2.new(1, -60, 1, 0)
        txt.BackgroundTransparency = 1
        txt.Text = "Earned +" .. amount
        txt.TextColor3 = Color3.new(1,1,1)
        txt.Font = Enum.Font.GothamBold
        txt.TextSize = 24
        txt.TextXAlignment = Enum.TextXAlignment.Left

        TweenService:Create(frame, TweenInfo.new(0.7), {Position = UDim2.new(0.5, -145, 0.15, 0)}):Play()
        task.wait(2.3)
        TweenService:Create(frame, TweenInfo.new(0.7), {Position = UDim2.new(0.5, -145, -0.2, 0)}):Play()
        task.wait(0.8)
        notif:Destroy()
    end)
end

