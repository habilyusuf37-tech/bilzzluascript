Instance.new("UICorner",mini)
local min = false
mini.MouseButton1Click:Connect(function() min = not min main.Size = min and UDim2.new(0,320,0,45) or UDim2.new(0,320,0,420) end)

-- Scroll
local scroll = Instance.new("ScrollingFrame",main)
scroll.Size = UDim2.new(1,-20,1,-65); scroll.Position = UDim2.new(0,10,0,55); scroll.BackgroundTransparency = 1
scroll.ScrollBarThickness = 5; scroll.CanvasSize = UDim2.new(0,0,0,750)
local list = Instance.new("UIListLayout",scroll); list.Padding = UDim.new(0,10)

-- Toggle & Slider Function
local function toggle(n,cb)
    local f = Instance.new("Frame",scroll); f.Size = UDim2.new(1,0,0,44); f.BackgroundColor3 = Color3.fromRGB(28,28,50)
    Instance.new("UICorner",f).CornerRadius = UDim.new(0,10)
    local txt = Instance.new("TextLabel",f); txt.Size = UDim2.new(0.65,0,1,0); txt.Position = UDim2.new(0,12,0,0)
    txt.Text = n; txt.TextColor3 = Color3.new(1,1,1); txt.TextXAlignment = Enum.TextXAlignment.Left; txt.Font = Enum.Font.GothamSemibold; txt.TextSize = 16; txt.BackgroundTransparency = 1
    local btn = Instance.new("TextButton",f); btn.Size = UDim2.new(0,55,0,28); btn.Position = UDim2.new(1,-68,0.5,-14)
    btn.Text = "OFF"; btn.BackgroundColor3 = Color3.fromRGB(90,0,0); Instance.new("UICorner",btn)
    local s = false
    btn.MouseButton1Click:Connect(function() s = not s btn.Text = s and "ON" or "OFF" btn.BackgroundColor3 = s and Color3.fromRGB(0,160,0) or Color3.fromRGB(90,0,0) cb(s) end)
end

local function slider(n,min,max,def,cb)
    local f = Instance.new("Frame",scroll); f.Size = UDim2.new(1,0,0,60); f.BackgroundColor3 = Color3.fromRGB(28,28,50)
    Instance.new("UICorner",f).CornerRadius = UDim.new(0,10)
    local l = Instance.new("TextLabel",f); l.Size = UDim2.new(1,0,0,28); l.Text = n..": "..def; l.TextColor3 = Color3.new(1,1,1); l.Font = Enum.Font.GothamSemibold; l.TextSize = 15; l.BackgroundTransparency = 1
    local bar = Instance.new("Frame",f); bar.Size = UDim2.new(0.9,0,0,8); bar.Position = UDim2.new(0.05,0,0.6,0); bar.BackgroundColor3 = Color3.fromRGB(50,50,70); Instance.new("UICorner",bar)
    local fill = Instance.new("Frame",bar); fill.Size = UDim2.new((def-min)/(max-min),0,1,0); fill.BackgroundColor3 = Color3.fromRGB(0,190,255); Instance.new("UICorner",fill)
    local drag = false
    bar.InputBegan:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 then drag = true end end)
    bar.InputEnded:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 then drag = false end end)
    UserInputService.InputChanged:Connect(function(i)
        if drag and i.UserInputType == Enum.UserInputType.MouseMovement then
            local p = math.clamp((i.Position.X - bar.AbsolutePosition.X)/bar.AbsoluteSize.X,0,1)
            local v = math.floor(min + (max-min)*p)
            fill.Size = UDim2.new(p,0,1,0); l.Text = n..": "..v; cb(v)
        end
    end)
end

-- Variables
local autofarm = false; local fly = false; local noclip = false
local ws = 16; local jp = 50; local delay = 8; local flyspeed = 80

-- WAYPOINT 100% ASLI KAMU
local Locations = {
    Vector3.new(2879, 4, -863),
    Vector3.new(-5, 4, 25),
    Vector3.new(3141, 5, -111)
}

-- AUTOFARM
local function startAF()
    if autofarm then return end
    autofarm = true
    task.spawn(function()
        local i = 1
        while autofarm and root do
            root.CFrame = CFrame.new(Locations[i] + Vector3.new(0,5,0))
            task.wait(0.8)
            VirtualInputManager:SendKeyEvent(true,Enum.KeyCode.E,false,game)
            task.wait(7.3)
            VirtualInputManager:SendKeyEvent(false,Enum.KeyCode.E,false,game)
            cashNotif("$"..string.format("%05d",math.random(1900,3400)))
            task.wait(delay)
            i = i + 1 if i > #Locations then i = 1 end
        end
    end)
end

-- FITUR DI MENU (PASTI MUNCUL)
toggle("Auto Farm", function(s) if s then startAF() else autofarm = false end end)
toggle("Fly (Space)", function(s) fly = s end)
toggle("Noclip (N)", function(s) noclip = s end)
slider("WalkSpeed",16,200,60,function(v) ws=v humanoid.WalkSpeed=v end)
slider("JumpPower",50,300,120,function(v) jp=v humanoid.JumpPower=v end)
slider("Farm Delay",5,20,8,function(v) delay=v end)
slider("Fly Speed",30,150,80,function(v) flyspeed=v end)

-- FLY & NOCLIP LOGIC
mouse.KeyDown:Connect(function(k)
    if fly and k==" " and root then
        local bv = Instance.new("BodyVelocity",root)
        bv.Velocity = Vector3.new(0,flyspeed,0); bv.MaxForce = Vector3.new(0,9e9,0)
        task.wait(0.18); bv:Destroy()
    elseif k:lower()=="n" then noclip = not noclip
    end
end)

RunService.Stepped:Connect(function()
    if noclip and character then
        for _,v in character:GetDescendants() do
            if v:IsA("BasePart") then v.CanCollide = false end
        end
    end
end)

-- Circle Premium
local cir = Instance.new("ImageLabel",gui)
cir.Size = UDim2.new(0,90,0,90); cir.Position = UDim2.new(1,-110,0,10)
cir.BackgroundTransparency = 1; cir.Image = "rbxassetid://4483345998"
cir.ImageColor3 = Color3.fromRGB(0,200,255)
Instance.new("UIStroke",cir).Thickness = 3; Instance.new("UIStroke",cir).Color = Color3.fromRGB(0,255,255); Instance.new("UIStroke",cir).Transparency = 0.6

